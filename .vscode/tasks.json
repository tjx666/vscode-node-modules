{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "preRunExtension",
			"type": "npm",
			"script": "esbuild-watch",
			"isBackground": true,
			"problemMatcher": "$esbuild-watch",
			"presentation": {
				"reveal": "never"
			}
		},
		{
			"label": "preTestExtension",
			"type": "npm",
			"script": "test-compile",
			"isBackground": true,
			"problemMatcher": "$esbuild",
			"presentation": {
				"reveal": "never"
			}
		},
	]
}
